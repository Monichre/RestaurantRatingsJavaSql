<div class="restaurant">
  #if ($restaurants)
    #foreach ($restaurant in $restaurants)
      <h2>$restaurant.getName()</h2>
      <div class="info">
        <p>$restaurant.getRating()</p>
        <p>$restaurant.getReview()</p>
      </div>
    #end
  #else
    <p>
    No new tests
    </p>
  #end
</div>
<button><a href="/">home</a></button>
